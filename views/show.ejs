<% include partials/header%>
<% include partials/nav%>

<p> <%= title %></p>

<table class="table">
  <thead>
    <tr>
      <th>Pika kufitare</th>
      <th>Hyrje min minuta</th>
      <th>Hyrje max minuta</th>
       <th>Dalje min minuta</th>
