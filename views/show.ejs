       <td><%= docs[i].dalje_min_metra %></td>
       <td><%= docs[i].dalje_max_metra %></td>
       <td><a href ="/edit/<%= docs[i]._id %>" >Edit </a>
       </td>
       <td><a href ="/delete/<%= docs[i]._id %>" >Delete </a>
       </td>
    </tr>
      <% } %>
 
  </tbody>
